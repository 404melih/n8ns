      "parameters": {
        "jsCode": "const items = $input.all();\n\nconst cleaned = items.map(item => {\n  const list = item.json.list;\n\n  const title = list[0].trim();\n  const content = list.slice(1).map(el => el.trim()).filter(el => el.length > 0).join(' ');\n\n  const text = `${title}\\n${content}`;\n\n  return { json: { text } };\n});\n\nreturn cleaned;\n"
